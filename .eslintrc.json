{
  "extends": "airbnb",
  "settings": {
    "import/resolver": {
      "webpack": {
        "config": "./webpack/webpack.base.js"
      }
    }
  },
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": ["error", {
      "singleQuote": true,
      "trailingComma": "all"
    }],
    "arrow-parens": "off"
  },
  "env": {
    "browser": true,
    "node": true
  }
}
